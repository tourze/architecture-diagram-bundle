{
  "name": "tourze/architecture-diagram-bundle",
  "description": "自动生成 Symfony 项目架构图的 Bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "nikic/php-parser": "^5.5.0",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/finder": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ArchitectureDiagramBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ArchitectureDiagramBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}